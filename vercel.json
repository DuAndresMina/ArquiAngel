{
  "redirects": [
    {
      "source": "/services/basic-service",
      "destination": "/services/servicio-basico",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
