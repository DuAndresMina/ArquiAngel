{
  "redirects": [
    {
      "source": "/((?!api).*)",
      "has": [
        { "type": "host", "value": "acarquitectura.com.co" }
      ],
      "destination": "https://www.acarquitectura.com.co/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
